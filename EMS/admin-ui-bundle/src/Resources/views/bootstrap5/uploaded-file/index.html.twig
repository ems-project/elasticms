{% extends '@EMSAdminUI/bootstrap5/base/admin.html.twig' %}
{% trans_default_domain 'EMSCoreBundle' %}

{% block title %}{{ 'uploaded-file.index.title'|trans }}{% endblock %}
{% block pagetitle %}{{ block('title') }}{% endblock %}

{% block breadcrumb %}
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><i class="{{ 'view.icon.class.uploaded-file'|trans }}"></i> {{ 'uploaded-file.menu'|trans }}</li>
    </ol>
{% endblock %}

{% block body %}

    {% form_theme form '@EMSAdminUI/bootstrap5/form/forms.html.twig' %}
    {{ form(form, {
        reorder_label: 'uploaded-file.index.reorder'
    }) }}
{% endblock %}
