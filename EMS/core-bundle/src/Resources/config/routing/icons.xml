<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing https://symfony.com/schema/routing/routing-1.0.xsd">

  <route id="favicon" path="favicon.ico"
         controller="EMS\CommonBundle\Controller\FileController::buildAssetImage"
         methods="GET|HEAD">
    <default key="filename">@EMSCoreBundle/Resources/public/images/big-logo.png</default>
    <default key="config">{"_width": 128, "_height": 128, "_quality": 0}</default>
  </route>
</routes>
