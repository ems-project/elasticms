{% extends '@EMSCH/template/variables.twig' %}

{%- block request %}
    {% apply spaceless %}
        {{ {
            path: emsch_asset('img/head/icon.png', {
                _config_type: 'image',
                _image_format: 'png',
                _width: app.request.get('width'),
                _height: app.request.get('height'),
                _get_file_path: true,
            }),
        }|json_encode|raw }}
    {% endapply %}
{% endblock request -%}

