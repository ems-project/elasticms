{
  "name": "elasticms/form-bundle",
  "description": "Generate forms based on elasticms configurations",
  "type": "symfony-bundle",
  "keywords": [
    "elasticms"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "EMS Community",
      "homepage": "https://github.com/ems-project/EMSFormBundle/contributors"
    }
  ],
  "require": {
    "php": "^8.2",
    "elasticms/client-helper-bundle": "6.0.*",
    "giggsey/libphonenumber-for-php": "^8.13",
    "symfony/form": "6.4.*",
    "symfony/intl": "6.4.*",
    "symfony/security-csrf": "6.4.*",
    "symfony/twig-bundle": "6.4.*",
    "symfony/validator": "6.4.*"
  },
  "require-dev": {
    "symfony/test-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "EMS\\FormBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EMS\\FormBundle\\Tests\\": "tests/"
    }
  },
  "config": {
    "lock": false,
    "sort-packages": true
  },
  "scripts": {
    "phpunit": "phpunit"
  }
}
