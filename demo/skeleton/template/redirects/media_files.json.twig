{%- block request %}
    {% apply spaceless %}
        {{ {
            controller: 'EMS\\CommonBundle\\Controller\\FileController::view',
            path: {
               sha1: source.media_file.sha1,
            },
            query: {
                name: source.media_file.filename|default(null),
                type: source.media_file.mimetype|default(null),
            },
        }|json_encode|raw }}
    {% endapply %}
{% endblock request -%}
