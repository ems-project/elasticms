                "text": "<div class=\"box\">\r\n    <div class=\"box-header with-border\">\r\n        <h3 class=\"box-title\">Documents</h3>\r\n    </div>\r\n    <div class=\"box-body\" style=\"display: block;\">\r\n        {{ emsco_datatable(['preview'],['asset'], {\r\n            \"empty_query\": {\r\n                \"bool\": {\r\n                    \"must\": [\r\n                        {\r\n                            \"term\": {\r\n                                \"type\": \"manual\"\r\n                            }\r\n                        }\r\n                    ]\r\n                }\r\n            },\r\n            \"query\": {\r\n                \"bool\": {\r\n                    \"must\": [\r\n                        {\r\n                            \"multi_match\": {\r\n                                \"fields\": [\r\n                                    \"live_search\",\r\n                                    \"live_search._2gram\",\r\n                                    \"live_search._3gram\"\r\n                                ],\r\n                                \"query\": \"%query%\",\r\n                                \"operator\": \"and\",\r\n                                \"type\": \"bool_prefix\"\r\n                            }\r\n                        },\r\n                        {\r\n                            \"term\": {\r\n                                \"type\": \"manual\"\r\n                            }\r\n                        }\r\n                    ]\r\n                }\r\n            },\r\n            \"columns\": [{\r\n                \"label\": \"Name\",\r\n                \"template\": '<a href=\"{{ ems_asset_path(data.source.file) }}\">{{ data.source.label|default(\"N/A\") }}</a>',\r\n                \"orderField\": \"label.alpha_order\"\r\n            },{\r\n                \"label\": \"Type\",\r\n                \"template\": '{{ (\"asset.type.\"~data.source.type)|emsco_i18n }}',\r\n                \"orderField\": \"type\"\r\n            },{\r\n                \"label\": \"Locales\",\r\n                \"template\": '{{ data.source.locales|default([])|map(locale => (\"locale.\"~locale)|i18n)|join(\", \") }}'\r\n            }]\r\n        }) }}\r\n    </div>\r\n</div>"
            }
        ]
